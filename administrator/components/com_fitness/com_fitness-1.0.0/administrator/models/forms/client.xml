<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="COM_FITNESS_FORM_LBL_CLIENT_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 
              
       <field name="business_profile_id" type="text" class="inputbox"
            label="Business Profile"
       /> 

       
       <field name="user_id" type="sql" class="inputbox"
            query="SELECT id, username FROM #__users"
            key_field="id"
            value_field="username"
            label="Username"
       /> 

       
       <field name="primary_trainer" type="text" class="inputbox"
            label="COM_FITNESS_FORM_LBL_CLIENT_PRIMARY_TRAINER"
       /> 
       
       
      <field name="other_trainers" type="text" class="inputbox"
            label="COM_FITNESS_FORM_LBL_CLIENT_OTHER_TRAINERS"
      /> 




         <field
        name="state"
        type="list"
        label="JSTATUS"
        description="JFIELD_PUBLISHED_DESC"
        class="inputbox"
        size="1"
        default="1">
        <option value="1">JPUBLISHED</option>
        <option value="0">JUNPUBLISHED</option>
    </field>

	</fieldset>

</form>
